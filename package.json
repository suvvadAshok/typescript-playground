{
  "name": "fullstack-training",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "build": "vite build",
    "dev": "vite",
    "test:dev": "vitest",
    "test": "vitest run --passWithNoTests",
    "cov": "vitest run --coverage",
    "cov:dev": "vitest watch --coverage",
    "types": "tsc --noEmit -p .",
    "format": "pnpm prettier --write --ignore-unknown .",
    "types:dev": "tsc --noEmit -p . --watch",
    "prepare": "husky install",
    "cd": "turbo types lint test",
    "html": "parcel ./pages/*.html --lazy"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "immer": "10.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "tsx": "^3.12.7",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/coverage-c8": "0.30.1",
    "buffer": "6.0.3",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "parcel": "2.8.3",
    "prettier": "2.8.7",
    "process": "0.11.10",
    "turbo": "1.9.3",
    "typescript": "5.0.4",
    "vite": "4.3.1",
    "vite-plugin-checker": "0.5.6",
    "vitest": "0.30.1"
  },
  "lint-staged": {
    "**/*": "pnpm format",
    "**/*.{ts,tsx}": "eslint --fix --cache --ext .ts,.tsx src"
  }
}
